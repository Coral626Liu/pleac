files=$(perl -e 'print join(" ", map { qq("$_") } @ARGV), "\n"' $*)
if [ -e ~/etc/emacs/htmlize.el ] ; then
  emacs --eval "(progn (htmlize-many-files '($files)) (kill-emacs))"
else 
  for inf in $files ; do
	if test ${inf:0:1} = "\"" ; then
	  inf=${inf:1}
	  inf=${inf%\"}
	fi
    outf=${inf%.*}.html${suffix}
    enscript -E --style=emacs_verbose --color -w html -o $outf $inf
  done
fi
